{
    "name": "faysal0x1/lara-payment",
    "description": "A Laravel Package that makes implementation of multiple payment Gateways endpoints and webhooks seamless",
    "keywords": [
        "Faysal0x1",
        "Cybernerdie",
        "laravel",
        "lara-payment",
        "Payments",
        "Paystack",
        "Laravel Paystack",
        "Flutterwave",
        "Laravel Package",
        "Laravel Multipayment Gateways",
        "Laravel Multi Payment Gateways",
        "Laravel Flutterwave",
        "Stripe",
        "Payment Gateway",
        "Paystack Laravel",
        "Multi Payment Gateways"
    ],
    "homepage": "https://github.com/faysal0x1/lara-payment",
    "license": "MIT",
    "authors": [
        {
            "name": "Faysal0x1",
            "email": "faysal.swe6@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/contracts": "^8.50|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.50|^9.0|^10.0|^11.0",
        "spatie/laravel-package-tools": "^1.11"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^5.3|^6.0|^v7.10|^8.1.1",
        "nunomaduro/larastan": "^1.0.4|^2.2",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0",
        "pestphp/pest": "^1.21|^2.34",
        "pestphp/pest-plugin-laravel": "^1.1|^v2.3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5|^10.4"
    },
    "autoload": {
        "files": [
            "src/Utils/Helpers.php"
        ],
        "psr-4": {
            "Faysal0x1\\LaravelMultipaymentGateways\\": "src",
            "Faysal0x1\\LaravelMultipaymentGateways\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Faysal0x1\\LaravelMultipaymentGateways\\Tests\\": "tests"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Faysal0x1\\LaravelMultipaymentGateways\\LaravelMultipaymentGatewaysServiceProvider"
            ],
            "aliases": {
                "LaravelMultipaymentGateways": "Faysal0x1\\LaravelMultipaymentGateways\\Facades\\LaravelMultipaymentGateways"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
